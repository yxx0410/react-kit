{
  "name": "react-kit",
  "version": "1.0.0",
  "description": "react-setup",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --open --history-api-fallback",
    "clean": "rimraf ./build/css/*.css && rimraf ./build/*.js",
    "build": "webpack --config webpack.prod.js --mode production",
    "s3sync": "aws s3 sync ./build *** --storage-class REDUCED_REDUNDANCY --delete",
    "deploy": "npm run clean && npm run build && npm run s3sync"
  },
